@page
@model Torneo.App.Frontend.Pages.Posiciones.CreateModel
@{
}
<h1>Nuevo posiciones</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="posicion.Nombre"></label>
        <input asp-for="posicion.Nombre" class="form-control">
        <span class="text-danger" asp-validation-for="posicion.Nombre"></span>
    </div>
    <button type="submit" class="btn btn-outline-warning btn-block">Crear posicion</button>
    <a class="btn btn-outline-info" asp-page="./Index">Volver</a>
</form>

